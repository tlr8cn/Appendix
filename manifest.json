{
  "manifest_version": 2,

  "name": "Appendix Plugin",
  "description": "This extension adds a query parameter to the current URL",
  "version": "1.0",
  "background": {
    "persistent": false,
    "scripts": ["main.js"]
  },
  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": [
   "activeTab",
   "tabs"
  ]
}
